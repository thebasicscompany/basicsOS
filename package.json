{
  "name": "basicsos",
  "private": true,
  "version": "0.1.0",
  "description": "BasicOS â€” Open-source company operating system",
  "workspaces": [
    "apps/*",
    "apps/mcp/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "start": "turbo run start",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,json}\" --ignore-path .gitignore",
    "lint:fix": "eslint . --fix --ext .ts,.tsx,.js,.jsx",
    "db:migrate": "bun --filter @basicsos/db migrate",
    "db:rls": "bun --filter @basicsos/db rls",
    "db:seed": "bun --filter @basicsos/db seed",
    "db:studio": "bun --filter @basicsos/db studio",
    "gen:module": "plop",
    "setup:select": "bun scripts/create-basicos.ts",
    "setup": "bun scripts/setup-wizard.ts",
    "dev:setup": "bun scripts/dev-setup.ts",
    "dev:setup:bash": "bash scripts/dev-setup.sh",
    "assets:sync": "bun scripts/sync-assets.ts",
    "db:generate": "bun --filter @basicsos/db generate"
  },
  "devDependencies": {
    "turbo": "^2.3.0",
    "prettier": "^3.3.0",
    "eslint": "^9.0.0",
    "@eslint/js": "^9.0.0",
    "typescript-eslint": "^8.0.0",
    "eslint-config-prettier": "^9.1.0",
    "typescript": "^5.6.0",
    "@types/node": "^22.0.0",
    "plop": "^4.0.1",
    "vitest": "^2.1.0",
    "@clack/prompts": "^0.9.0",
    "picocolors": "^1.1.1"
  },
  "overrides": {
    "ioredis": "5.4.0",
    "drizzle-orm": "^0.45.1",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0"
  },
  "packageManager": "bun@1.2.0",
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.1.0"
  },
  "dependencies": {
    "recharts": "^3.7.0"
  }
}
